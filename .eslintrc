{
    "extends": "airbnb",
    "globals": {
        "document": true,
        "fetch": true,
    },
    "env": {
        "browser": true,
    },
    "rules": {
      "linebreak-style": 0,
      "import/extensions": [0, "always"],
      "jsx-a11y/no-static-element-interactions": [0, "always"],
      "import/no-extraneous-dependencies": [error, { devDependencies: true }],
      "no-underscore-dangle": 0,
      "react/forbid-prop-types": 0,
    }
}
